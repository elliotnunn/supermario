		MACRO
		_TFSCORE
		IF	   &SYSLIST[1] <> '' THEN
		LEA    &SYSLIST[1],A0
		ENDIF
		IF &SYSLIST[3] = '' THEN
		DC.W   $A200+&SYSLIST[2]
		ENDIF
		IF &SYSLIST[3] = 'REGS' THEN
		DC.W   $A300+&SYSLIST[2]
		ENDIF
		IF &SYSLIST[3] = 'ASYNC' THEN
		DC.W   $A600+&SYSLIST[2]
		ENDIF
		IF &SYSLIST[3] = 'SYS' THEN
		DC.W   $A600+&SYSLIST[2]
		ENDIF
		IF &SYSLIST[3] = 'IMMED' THEN
		DC.W   $A200+&SYSLIST[2]
		ENDIF

		ENDM

